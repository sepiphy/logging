{
    "name": "sepiphy/logging",
    "description": "Support handlers out of monolog/monolog",
    "keywords": ["sepiphy", "tools", "phptools", "logging"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sepiphy/logging",
    "support": {
        "issues": "https://github.com/sepiphy/logging/issues",
        "source": "https://github.com/sepiphy/logging"
    },
    "authors": [
        {
            "name": "Quynh Xuan Nguyen",
            "email": "seriquynh@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "monolog/monolog": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5",
        "sun-asterisk/chatwork-php": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "Sepiphy\\Logging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Sepiphy\\Logging\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "sun-asterisk/chatwork-php": "Required to use ChatworkHandler (^0.2.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
